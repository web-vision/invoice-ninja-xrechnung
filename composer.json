{
    "name": "webvision/ninja-xrechnung",
    "version": "1.0.1",
    "description": "A module to exchange UBL generator with zugferd in invoice ninja",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Bushra Asif",
            "email": "bushra@web-vision.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "easybill/zugferd-php": "^1.7",
        "cleverit/ubl_invoice": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Webvision\\NinjaZugferd\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan migrate:ninja-xrechnung --force"
        ],
        "post-install-cmd": [
            "@php artisan migrate:ninja-xrechnung --force"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webvision\\NinjaZugferd\\Providers\\RouteServiceProvider"
            ]
        }
    }
}

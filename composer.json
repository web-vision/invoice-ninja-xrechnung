{
    "name": "webvision/invoice-ninja-xrechnung",
    "version": "1.0.0",
    "description": "Add-on module for Invoice Ninja 5.x to create electronic invoices for Germany's xRechnung format",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Web-Vision",
            "email": "bushra@web-vision.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "easybill/zugferd-php": "^1.7",
        "cleverit/ubl_invoice": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Webvision\\NinjaZugferd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webvision\\NinjaZugferd\\Providers\\RouteServiceProvider"
            ]
        }
    }
}
